{
  "name": "fncts",
  "packageManager": "yarn@4.0.0-rc.14",
  "type": "module",
  "license": "BSD 3-Clause",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/0x706b/fncts"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "build": "tsc -b --verbose",
    "build-pack": "yarn build && ultra -r build-pack",
    "format": "ultra -r format",
    "clean": "ultra -r clean",
    "test": "ultra -r test",
    "lint": "ultra -r lint",
    "circular": "madge --ts-config ./tsconfig.madge.json --circular --warning packages/*/build/esm",
    "postinstall": "echo tsplus-install",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:babel": "cd $INIT_CWD && babel",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:madge": "cd $INIT_CWD && madge"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-align-assignments": "^0.1.2",
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.1.8",
    "@0x706b/prettier-eslint-8": "^0.0.3",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@changesets/changelog-git": "^0.1.13",
    "@changesets/cli": "^2.25.2",
    "@tsplus/installer": "0.0.153",
    "@types/benchmark": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/experimental-utils": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "benchmark": "^2.1.4",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-codegen": "^0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "madge": "^5.0.1",
    "prettier": "^2.7.1",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.162/compiler/typescript.tgz",
    "ultra-runner": "^3.10.5"
  },
  "resolutions": {
    "@babel/types": "^7.20.2"
  }
}
