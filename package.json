{
  "name": "fncts",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "license": "BSD 3-Clause",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/0x706b/fncts"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "build": "tsc -b --verbose",
    "build-pack": "yarn build && ultra -r build-pack",
    "format": "ultra -r format",
    "clean": "ultra -r clean",
    "test": "ultra -r test",
    "circular": "madge --ts-config ./tsconfig.madge.json --circular --warning packages/*/build/esm",
    "postinstall": "tsplus-install",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:concurrently": "cd $INIT_CWD && concurrently"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-align-assignments": "^0.1.2",
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.1.7",
    "@0x706b/prettier-eslint-8": "^0.0.3",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@changesets/changelog-git": "^0.1.11",
    "@changesets/cli": "^2.22.0",
    "@tsplus/installer": "0.0.102",
    "@types/benchmark": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/experimental-utils": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.2",
    "benchmark": "^2.1.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-codegen": "^0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "madge": "^5.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.2",
    "ultra-runner": "^3.10.5"
  }
}
