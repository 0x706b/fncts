{
  "name": "fncts",
  "packageManager": "yarn@4.0.0-rc.11",
  "type": "module",
  "license": "BSD 3-Clause",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/0x706b/fncts"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "build": "tsc -b --verbose",
    "build-pack": "yarn build && ultra -r build-pack",
    "format": "ultra -r format",
    "clean": "ultra -r clean",
    "test": "ultra -r test",
    "circular": "madge --ts-config ./tsconfig.madge.json --circular --warning packages/*/build/esm",
    "postinstall": "tsplus-install",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:concurrently": "cd $INIT_CWD && concurrently"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-align-assignments": "^0.1.2",
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.1.7",
    "@0x706b/prettier-eslint-8": "^0.0.3",
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@changesets/changelog-git": "^0.1.11",
    "@changesets/cli": "^2.23.0",
    "@tsplus/installer": "0.0.126",
    "@types/benchmark": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/experimental-utils": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "benchmark": "^2.1.4",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-codegen": "^0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "madge": "^5.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "ultra-runner": "^3.10.5"
  }
}
