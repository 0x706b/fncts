{
  "name": "fncts",
  "packageManager": "yarn@4.0.0-rc.39",
  "type": "module",
  "license": "BSD 3-Clause",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/0x706b/fncts"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "build": "tsc -b --verbose",
    "build-pack": "yarn build && ultra -r build-pack",
    "format": "ultra -r format",
    "clean": "ultra -r clean",
    "test": "yarn build && yarn vitest",
    "lint": "ultra -r lint",
    "circular": "madge --ts-config ./tsconfig.madge.json --circular --warning packages/*/build/esm",
    "postinstall": "echo tsplus-install",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:babel": "cd $INIT_CWD && babel",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:madge": "cd $INIT_CWD && madge",
    "org:vitest": "cd $INIT_CWD && vitest"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-align-assignments": "^0.1.2",
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.1.8",
    "@0x706b/prettier-eslint-8": "^0.0.3",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/cli": "^2.26.1",
    "@tsplus/installer": "0.0.173",
    "@types/benchmark": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/experimental-utils": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "benchmark": "^2.1.4",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-codegen": "^0.17.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "madge": "^6.0.0",
    "prettier": "^2.8.7",
    "typescript": "https://github.com/0x706b/typescript/raw/aad153071b/typescript-5.1.0-tsplus.20230422.tgz",
    "ultra-runner": "^3.10.5",
    "vitest": "^0.30.1"
  },
  "resolutions": {
    "@babel/types": "7.21.4"
  }
}
