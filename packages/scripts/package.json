{
  "name": "@fncts/scripts",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "bin": {
    "fncts-pack": "./bin/fncts-pack.js"
  },
  "exports": {
    "./*": "./build/esm/*.js",
    "./*.js": "./build/esm/*.js"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "format": "prettier --list-different -w src/**/*.ts && eslint --fix src/**/*.ts",
    "circular": "madge --circular --no-spinner --warning build/esm",
    "clean": "rm -rf ./build"
  },
  "devDependencies": {
    "@fncts/config": "workspace:*",
    "@fncts/eslint-config": "workspace:*",
    "@yarnpkg/cli": "4.0.0-rc.6",
    "@yarnpkg/core": "4.0.0-rc.6",
    "madge": "^5.0.1",
    "typescript": "^4.7.2"
  }
}
