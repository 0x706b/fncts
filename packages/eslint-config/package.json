{
  "name": "@fncts/eslint-config",
  "version": "0.0.13",
  "scripts": {
    "build": "rm -rf build && mkdir build && mkdir build/cjs && cp index.cjs build/cjs/index.cjs",
    "build-pack": "yarn build && yarn fncts-pack cjs"
  },
  "dependencies": {
    "@0x706b/eslint-plugin-align-assignments": "^0.1.2",
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.1.8",
    "@fncts/codegen": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/experimental-utils": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-codegen": "^0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.21.0"
  },
  "devDependencies": {
    "@fncts/config": "workspace:*"
  },
  "exports": {
    ".": {
      "require": "./index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
