{
  "compilerOptions": {
    "strict": true,
    "downlevelIteration": true,
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["ES2020", "ESNext.WeakRef"],
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "paths": {
      "@fncts/typelevel/*": ["../typelevel/build/dts/*"]
    }
  }
}
