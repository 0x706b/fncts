{
  "compilerOptions": {
    "moduleDetection": "force",
    "composite": true,
    "strict": true,
    "downlevelIteration": true,
    "module": "ESNext",
    "target": "ES2020",
    "preserveSymlinks": true,
    "lib": ["ES2021", "ESNext.WeakRef"],
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@fncts/typelevel/*": ["../typelevel/src/*"],
      "@fncts/base/*": ["../base/src/*"],
      "@fncts/test/*": ["../test/src/*"],
      "@fncts/io/*": ["../io/src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": []
}
